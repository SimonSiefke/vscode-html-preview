{
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/dist": false,
    "**/.vscode": false,
    "deprecated": false,
    "**/node_modules": false,
    "playground": false,
    "webpack": false,
    ".gitignore": false,
    ".travis.yml": false,
    "CHANGELOG.md": false,
    "lerna.json": false,
    "LICENSE": false,
    "**/package-lock.json": false,
    "ThirdPartyNotices.txt": false,
    "tsconfig-base.json": false,
    "tsconfig-noncomposite-base.json": false,
    "tsconfig.json": false,
    "scripts": false,
    "**/tsconfig.tsbuildinfo": false
  },
  "files.trimTrailingWhitespace": false,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true
}
