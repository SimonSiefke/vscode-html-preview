{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/dist": true,
    "**/.vscode": true,
    "deprecated": true,
    "**/node_modules": true,
    "playground": true,
    "webpack": true,
    ".gitignore": true,
    ".travis.yml": true,
    "CHANGELOG.md": true,
    "lerna.json": true,
    "LICENSE": true,
    "**/package-lock.json": true,
    "ThirdPartyNotices.txt": true,
    "tsconfig-base.json": true,
    "tsconfig-noncomposite-base.json": true,
    "tsconfig.json": true,
    "scripts": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "files.trimTrailingWhitespace": false,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true
}
