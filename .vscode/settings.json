{
  "[html]": {
    "editor.formatOnSave": false
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/dist": false,
    "**/.vscode": false,
    "deprecated": false,
    "**/node_modules": false,
    "playground": false,
    "webpack": false,
    ".gitignore": false,
    ".travis.yml": false,
    "CHANGELOG.md": false,
    "lerna.json": false,
    "LICENSE": false,
    "**/package-lock.json": false,
    "ThirdPartyNotices.txt": false,
    "tsconfig-base.json": false,
    "tsconfig-noncomposite-base.json": false,
    "tsconfig.json": false,
    "scripts": false,
    "**/tsconfig.tsbuildinfo": false,
    "i18n": false,
    "**/.rpt2_cache": false,
    "**/.vscode-test": false,
    "debug.log": false,
    "global.d.ts": false,
    "open.js": false,
    "perfomance-observer.html": false,
    "process.js": false,
    "tempdir.js": false,
    "**/rollup.config.js": false,
    "**/images": false,
    "**/(deprecated)*": false,
    "examples": false,
    "**/html-preview-service": false,
    ".prettierignore": false,
    "**/tsconfig.json": false,
    "**/README.md": false
  },
  "search.exclude": {
    "**/node_modules": false,
    "**/bower_components": false,
    "**/dist": false
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true
  },
  "files.trimTrailingWhitespace": false,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "json.schemas": [
    {
      "fileMatch": [
        "i18n/de.json",
        "i18n/en.json",
        "i18n/es.json",
        "i18n/fr.json",
        "i18n/it.json",
        "i18n/ja.json",
        "i18n/nl.json",
        "i18n/ru.json",
        "i18n/zh-cn.json"
      ],
      "url": "./i18n/_schema.json"
    }
  ]
}
